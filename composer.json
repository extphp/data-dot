{
    "name": "extphp/data-dot",
    "description": "A property accessor helper capable of using dot notation for a mix of arrays and objects.",
    "type": "library",
    "keywords": [ "dot", "notation", "access", "data" ],
    "homepage": "https://github.com/extphp/data-dot",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0|^7.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "ExtPHP\\DataDot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
